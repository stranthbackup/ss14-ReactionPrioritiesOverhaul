handheld-gps-coordinates = [color=white]Mode:[/color] {$mode ->
        *[false] Space coords
        [true] Station coords
    }
    {"[color=white]Position:[/color]"} {$coords}
    {$keybind} to switch mode

handheld-gps-coordinates-unknown = no signal
handheld-gps-coordinates-no-bind = (unknown)
